    "intelephense.diagnostics.undefinedVariables": true,
    "intelephense.diagnostics.deprecated": true,
    "intelephense.diagnostics.undefinedTypes": true,
    "intelephense.diagnostics.languageConstraints": true,
    "vetur.enable": false,
    "volar.enable": true,
    "volar.autoClosingTags": true,
    "volar.formatting.enable": true,
    "volar.scaffoldSnippets.enable": true,
    "volar.completion.autoImportComponent": true,
    "volar.autoCompleteRefs": true,
    "volar.autoCreateQuotes": true,
    "volar.codeLens.scriptSetupTools": true
}
