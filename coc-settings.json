    "suggest.autoTrigger": "always",
    "suggest.echodocSupport": true,
    "suggest.noselect": true,
    "suggest.completionItemKindLabels": {
        "class": "CLASS ",
        "color": "COLOR ",
        "constant": "CONSTANT ",
        "function": "FUNCTION",
        "default": "DEFAULT",
        "enum": "ENUM",
        "keyword": "KEYWORD",
        "method": "METHOD",
        "module": "MODULE",
        "operator": "OPERATTOR",
        "property": "PROPERTY",
        "reference": "REFERENCE",
        "snippet": "SNIPPET",
        "struct": "STRUCT",
        "text": "TEXT",
        "typeParameter": "TYPE_PARAMETER",
        "unit": "UNIT",
        "value": "VALUE",
        "variable": "VARIABLE"
    },
    "intelephense.progress.enable": false,
    "intelephense.diagnostics.typeErrors": true,
    "intelephense.diagnostics.undefinedVariables": true,
    "intelephense.diagnostics.deprecated": true,
